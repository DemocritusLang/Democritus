; ModuleID = 'MicroC'

%Circle = type <{ i32 }>

@fmt = private unnamed_addr constant [4 x i8] c"%d\0A\00"
@tmp = private unnamed_addr constant [13 x i8] c"hello world\0A\00"

declare i32 @printf(i8*, ...)

define i32 @main() {
entry:
  %b = alloca i1
  %y = alloca i32
  %x = alloca %Circle
  %printf = call i32 (i8*, ...)* @printf(i8* getelementptr inbounds ([13 x i8]* @tmp, i32 0, i32 0))
  ret i32 0
}
